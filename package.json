    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "nest-next": "^10.0.0",
    "next": "^12.1.6",
    "nodemon": "^2.0.18",
    "pg": "^8.7.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
